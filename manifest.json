{
  "short_name": "Dark Marvin",
  "name": "Moulinette Dark Mode",
  "description": "Better version of dark mode for Epitech moulinette (https://my.epitech.eu)",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "64": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "*://my.epitech.eu/*"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Moulinette Dark"
  },
  "version": "1.0"
}
